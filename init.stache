<div class="container-fluid">
	<div class="row">
		<div class="col-md-2">
			<w-menu current="{list}" url-type="list" items="{menuItems}"></w-menu>
		</div>
		<div class="{{#if detailsOpen}}col-md-5{{else}}col-md-10{{/if}}">
			<w-list state="{this}" name="{currentListName}" items="{currentListItems}"></w-list>
		</div>
		{{#if detailsOpen}}
			<div class="col-md-5">
				<w-details state="{this}"></w-details>
			</div>
		{{/if}}
	</div>
</div>
<w-compose state="{this}"><w-compose>